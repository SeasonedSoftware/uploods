{
  "name": "uploods",
  "version": "0.4.0",
  "description": "Uploods React component",
  "author": "SeasonedSoftware",
  "license": "MIT",
  "repository": "SeasonedSoftware/uploods",
  "main": "dist/uploods.cjs.js",
  "module": "dist/uploods.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "scaffold": "node ./scripts/scaffold.js",
    "build": "npm run build:types && npm run build:js",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "yarn build && cd example/ && yarn build && cd ../",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "browser-image-resizer": "^2.0.1",
    "prop-types": "^15.7.2",
    "react-dropzone": "^10.1.5"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "firebase": "^6.3.0",
    "lodash": "4.17.14",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "cross-env": "^5.2.0",
    "firebase": "^6.3.0",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.14",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "readline-promise": "^1.0.4",
    "replace-in-file": "^4.1.1",
    "rollup": "^1.16.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-uglify": "^6.0.2",
    "typescript": "^3.5.3"
  }
}
